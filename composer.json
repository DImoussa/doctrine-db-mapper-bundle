{
    "name": "dimoussa/doctrine-db-mapper-bundle",
    "description": "Bundle Symfony pour générer automatiquement des entités Doctrine depuis une base de données MySQL avec support complet des relations (OneToMany, ManyToOne, ManyToMany)",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "doctrine", "generator", "entity", "database", "mysql", "orm", "bundle"],
    "authors": [
        {
            "name": "Diallo Moussa",
            "email": "moussadou128@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/console": "^6.0|^7.0",
        "symfony/config": "^6.0|^7.0",
        "symfony/dependency-injection": "^6.0|^7.0",
        "symfony/http-kernel": "^6.0|^7.0",
        "symfony/process": "^6.0|^7.0",
        "doctrine/orm": "^2.14|^3.0",
        "doctrine/dbal": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Bundle\\DbMapperBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Bundle\\DbMapperBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

